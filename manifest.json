{
    "name": "Yamedia Downloader",
    "version": "2.2",
    "description": "Download media from Yam. 下載天空影音的音樂",
    "permissions": [
        "tabs",
        "http://mymedia.yam.com/*"
    ],
	"icons": {
        "16": "icon.png",
        "32": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    },
    "content_scripts": [{
        "matches": ["http://mymedia.yam.com/*"],
        "js": [
            "jquery-1.3.2.min.js",
            "jquery-ui-1.8.7.effect.min.js",
            "getMedia.js"
        ]
    }]
}